<?xml version="1.0" encoding="UTF-8"?>
<MOML>

	<FUNCTIONLIST>

		<FUNCTION id="toHome">
			<FUNCTIONITEM
				cmd="action:go(&apos;root.navigation&apos;,&apos;SerchMain&apos;)" />
		</FUNCTION>
		
		<FUNCTION id="toSerch">
			<FUNCTIONITEM
				cmd="action:go(&apos;root.navigation&apos;,&apos;SerchDetail&apos;)" />
		</FUNCTION>

		<FUNCTION id="toLocation">
			<FUNCTIONITEM
				cmd="action:go(&apos;root.navigation&apos;,&apos;SerchLocation&apos;)" />
		</FUNCTION>

		<FUNCTION id="toSerch">
			<FUNCTIONITEM
				cmd="action:go(&apos;root.navigation&apos;,&apos;SerchDetail&apos;)" />
		</FUNCTION>
		
		<FUNCTION id="toSetting">
			<FUNCTIONITEM
				cmd="action:go(&apos;root.navigation&apos;,&apos;Setting&apos;)" />
		</FUNCTION>

		<FUNCTION id="toTreasure">
			<FUNCTIONITEM
				cmd="action:go(&apos;root.navigation&apos;,&apos;TreasureInfo&apos;)" />
		</FUNCTION>
		
		<FUNCTION id="toMypage">
			<FUNCTIONITEM
				cmd="action:go(&apos;root.navigation&apos;,&apos;Mypage&apos;)" />
		</FUNCTION>

		<FUNCTION id="toNotice">
			<FUNCTIONITEM
				cmd="action:go(&apos;root.navigation&apos;,&apos;Notice&apos;)" />
		</FUNCTION>
		
		<FUNCTION id="toVersion">
			<FUNCTIONITEM
				cmd="action:go(&apos;root.navigation&apos;,&apos;Version&apos;)" />
		</FUNCTION>
		
		<FUNCTION id="toAccess">
			<FUNCTIONITEM
				cmd="action:go(&apos;root.navigation&apos;,&apos;Accessterms&apos;)" />
		</FUNCTION>
	

	</FUNCTIONLIST>

	<UI>

		<UILAYOUT landscape="1080,1920" portrait="1080,1920">

			<WINDOW align="relative" defaultImg="embed:/res/pri_24/bg.png"
				layout="0,0,1080,1920">

				<IMAGE defaultImg="embed:/res/pri_24/gray_box.png" layout="40,510,1000,900" />

				<WINDOW layout="40,510,500,450"> <!-- 공지사항 -->

					<BUTTON defaultImg="embed:/res/pri_24/notice_off.png"
						layout="97,54,306,306" onClick="{function.toNotice}" pressedImg="embed:/res/pri_24/notice_on.png" />

					<LABEL layout="0,375,500,auto" fontColor="#ffffff" fontSize="64"
						text="公告" textAlign='center' />
				</WINDOW>

				<WINDOW layout="540,510,500,450"> <!-- 알람받기 -->

					<BUTTON defaultImg="embed:/res/pri_24/inform_off.png"
						layout="97,54,306,306"  pressedImg="embed:/res/pri_24/inform_on.png" />

					<LABEL layout="0,375,500,auto" fontColor="#ffffff" fontSize="64"
						text="推送信息设置" textAlign='center' />
				</WINDOW>

				<WINDOW layout="40,954,500,450"> <!-- 개인정보보호 -->

					<BUTTON defaultImg="embed:/res/pri_24/personal_information_off.png"
						layout="97,54,306,306" onClick="{function.toAccess}"
						pressedImg="embed:/res/pri_24/personal_information_on.png" />

					<LABEL layout="0,375,500,auto" fontColor="#ffffff" fontSize="64"
						text="个人信息保护协议" textAlign='center' />
				</WINDOW>

				<WINDOW layout="540,954,500,450"> <!-- 버전 및 문의 -->

					<BUTTON defaultImg="embed:/res/pri_24/version_off.png"
						layout="97,54,306,306" onClick="{function.toVersion}" pressedImg="embed:/res/pri_24/version_on.png" />

					<LABEL layout="0,375,500,auto" fontColor="#ffffff" fontSize="64"
						text="版本及客服" textAlign='center' />
				</WINDOW>

				<WINDOW defaultImg="#ff0000" layout="0,0,1080,120">

					<LABEL fontColor="#ffffff" fontSize="72" layout="0,0,1080,120"
						text="设置" textAlign="center" />

					<BUTTON defaultImg="embed:/res/pri/home_button.png" layout="25,15,220,90"
						onClick="{function.toHome}" />

					<BUTTON defaultImg="embed:/res/pri/red_position_off.png"
						layout="815,10,100,100" prossedImg="embed:/res/pri/red_position_on.png"
						onClick="{function.toLocation}" />

					<BUTTON defaultImg="embed:/res/pri/red_search_off.png"
						layout="940,10,100,100" onClick="{function.toSerch}" prossedImg="embed:/res/pri/red_search_on.png" />
				</WINDOW>

				<WINDOW align="linear:horizontal" defaultImg="#ffffff"
					layout="0,1750,1080,170">

					<BUTTON defaultImg="embed:/res/pri/gray_search_off.png"
							layout="270,170" 
							onClick="{function.toSerch}" 
							pressedImg="embed:/res/pri/gray_search_on.png" />

					<BUTTON defaultImg="embed:/res/pri/gray_coupon_off.png"
						layout="270,170" pressedImg="embed:/res/pri/gray_coupon_on.png"
						onClick="{function.toMypage}" />

					<BUTTON defaultImg="embed:/res/pri/gray_treasure_off.png"
						layout="270,170" pressedImg="embed:/res/pri/gray_treasure_on.png"
						onClick="{function.toTreasure}" />

					<BUTTON defaultImg="embed:/res/pri/gray_set_off.png" layout="270,170"
						onClick="{function.toSetting}" />
				</WINDOW>
			</WINDOW>
		</UILAYOUT>
	</UI>

</MOML>