<?xml version="1.0" encoding="UTF-8"?>
<MOML>

    <FUNCTIONLIST>

        <FUNCTION id="init" >

            <CMD cmd="function.setJob(json.toXml(file.read(&apos;http://tourcoupon.net/api2/coupon/search/category.php?access_token=&apos;+userVariable.acces+&apos;&amp;category=&apos;+userVariable.cate+&apos;&amp;start=&apos;+userVariable.start)))" />
        </FUNCTION>

        <FUNCTION id="setJob(cXml)" >
            <CMD cmd="title=xpath.evaluateEx(cXml,&apos;/DATA/data/name/text()&apos;)" />
            <CMD cmd="title.text=title" />
            <CMD cmd="discount=xpath.evaluateEx(cXml,&apos;/DATA/data/sales/text()&apos;)" />
            <CMD cmd="discount.text=discount" />
            <CMD cmd="company=xpatch.evaluateEx(cXml,&apos;/DATA/data/company/text()&apos;)" />
            <CMD cmd="company.text=company" />
            <CMD cmd="business=xpatch.evaluateEx(cXml,&apos;/DATA/data/business/text()&apos;)" />
            <CMD cmd="business.text=business" />
            <CMD cmd="thumbnail=xpath.evaluateEx(cXml,&apos;/DATA/data/thumbnail/text()&apos;)" />
            <CMD cmd="thumbnail.defaultImg='http://tourcoupon.net/api2/upload/thumbnail/+thumbnail" />
        </FUNCTION>

        <FUNCTION id="toHome" >
            <FUNCTIONITEM cmd="action:go(&apos;root.navigation&apos;,&apos;SerchMain&apos;)" />
        </FUNCTION>

        <FUNCTION id="toSerch" >
            <FUNCTIONITEM cmd="action:go(&apos;root.navigation&apos;,&apos;SerchDetail&apos;)" />
        </FUNCTION>

        <FUNCTION id="toLocation" >
            <FUNCTIONITEM cmd="action:go(&apos;root.navigation&apos;,&apos;SerchLocation&apos;)" />
        </FUNCTION>
        
        <FUNCTION id="toDetail">
            <FUNCTIONITEM cmd="action:go(&apos;root.navigation&apos;,&apos;Detail&apos;)" />
        </FUNCTION>
    </FUNCTIONLIST>

