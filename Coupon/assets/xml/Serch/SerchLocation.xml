                    </IMAGE>

                    <IMAGE
                        align="relative"
                        defaultImg="embed:/res/pri_27/gray_bar.png"
                        layout="1080,151" >

                        <LABEL
                            fontColor="#ffffff"
                            fontSize="72"
                            layout="70,0,auto,151"
                            text="仁川市"
                            textAlign="center" />
                    </IMAGE>

                    <IMAGE
                        align="relative"
                        defaultImg="embed:/res/pri_27/gray_bar.png"
                        layout="1080,151" >

                        <LABEL
                            fontColor="#ffffff"
                            fontSize="72"
                            layout="70,0,auto,151"
                            text="济州岛"
