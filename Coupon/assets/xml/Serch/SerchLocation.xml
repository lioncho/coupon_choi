                        <LABEL
                            fontColor="#ffffff"
                            fontSize="72"
                            layout="70,0,auto,151"
                            text="釜山市"
                            textAlign="center" />
                    </IMAGE>

                    <IMAGE
                        align="relative"
                        defaultImg="embed:/res/pri_27/gray_bar.png"
                        layout="1080,151" >

                        <LABEL
                            fontColor="#ffffff"
                            fontSize="72"
                            layout="70,0,auto,151"
                            text=""
                            textAlign="center" />
                    </IMAGE>

                    <IMAGE
                        align="relative"
                        defaultImg="embed:/res/pri_27/gray_bar.png"
                        layout="1080,151" >

                        <LABEL
                            fontColor="#ffffff"
                            fontSize="72"
                            layout="70,0,auto,151"
                            text=""
                            textAlign="center" />
                    </IMAGE>
                </WINDOW>

                <WINDOW
                    defaultImg="#ff0000"
                    layout="0,0,1080,120" >

                    <LABEL
                        fontColor="#ffffff"
                        fontSize="72"
                        layout="0,0,1080,120"
                        text="按位置搜索"
                        textAlign="center" />

                    <BUTTON
                        defaultImg="embed:/res/pri/home_button.png"
                        layout="25,15,220,90"
                        onClick="{function.toHome}" />

                    <BUTTON
                        defaultImg="embed:/res/pri/red_position_off.png"
                        layout="815,10,100,100"
