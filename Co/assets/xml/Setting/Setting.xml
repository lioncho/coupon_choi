<?xml version="1.0" encoding="UTF-8"?>
<MOML>

    <FUNCTIONLIST>

        <FUNCTION id="toHome" >

            <FUNCTIONITEM cmd="action:go(&apos;root.navigation&apos;,&apos;SerchMain&apos;)" />

            <FUNCTION id="toSerch" >

                <FUNCTIONITEM cmd="action:go(&apos;root.navigation&apos;,&apos;SerchDetail&apos;)" />
            </FUNCTION>
        </FUNCTION>
    </FUNCTIONLIST>

    <UI>

        <UILAYOUT
            landscape="1080,1920"
            portrait="1080,1920" >

            <WINDOW
                align="relative"
                defaultImg="embed:/res/pri_24/bg.png"
