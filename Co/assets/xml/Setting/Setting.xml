                <IMAGE
                    defaultImg="embed:/res/pri_24/gray_box.png"
                    layout="40,365,1000,900" />

                <WINDOW
                    align="linear:vertical|center"
                    layout="40,230,500,363" >

                    <BUTTON
                        defaultImg="embed:/res/pri_6/business_off.png"
                        layout="250,250"
                        onClick="{function.toJob}"
                        pressedImg="embed:/res/pri_6/business_on.png" />
                </WINDOW>

                <WINDOW
                    align="linear:vertical|center"
                    layout="540,230,500,363" >

                    <BUTTON
                        defaultImg="embed:/res/pri_6/position_off.png"
                        layout="250,250"
                        pressedImg="embed:/res/pri_6/position_on.png" />
                </WINDOW>

                <WINDOW
                    align="linear:vertical|center"
                    layout="40,594,500,363" >

                    <BUTTON
                        defaultImg="embed:/res/pri_6/favorite_off.png"
                        layout="250,250"
                        pressedImg="embed:/res/pri_6/favorite_on.png" />
                </WINDOW>

                <WINDOW
                    align="linear:vertical|center"
                    layout="540,594,500,363" >

                    <BUTTON
                        defaultImg="embed:/res/pri_6/recommend_off.png"
                        layout="250,250"
                        pressedImg="embed:/res/pri_6/recommend_on.png" />
